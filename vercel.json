{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/api/:path*"
    }
  ],
  "images": {
    "dangerouslyAllowSVG": true,
    "sizes": [256, 640, 1080, 2048, 3840],
    "remotePatterns": []
  },
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "src/*.*"
    }
  },
  "public": true
}
